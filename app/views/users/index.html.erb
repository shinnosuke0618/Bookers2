<div class='container px-5 px-sm-0'>
  <div class='row'>
<%= render 'books/list', book: @book %>

<div class='col-md-8 offset-md-1'>
<h2>Users</h2>
<table class='table table-hover table-inverse'>
    <thead>
      <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
      <% @users.each do |user| %>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td>
          <%= image_tag user.get_profile_image %>
        </td>
        <td>
          <%= user.name %>
        </td>
        <td>
          <%= link_to"Show", user_path(user.id) %>
        </td>
        <% end %>
        </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>